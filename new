<div class="row product-grid-4">
    <% products.forEach(product => { %>  
        <div class="col-lg-3 col-md-4 col-12 col-sm-6">
            <div class="product-cart-wrap mb-30">
                <div class="product-img-action-wrap">
                    <div class="product-img product-img-zoom">
                        
                            <img class="default-img" src="<%= product.image[0] %>" alt="">
                            <img class="hover-img" src="<%= product.image[1] %>" alt="">
                        
                    </div>
                </div>
                <div class="product-content-wrap">
                    <div class="product-category">
                        <a href="/category/<%= product.category._id %>"><%= product.category.name %></a>
                    </div>
                    <h2><a href="/product/<%= product._id %>"><%= product.name %></a></h2>
                    
                    <div class="product-price">
                        <span>$<%= product.mrp %> </span>
                        <% if (product.mrp) { %>
                            <span class="old-price">$<%= product.mrp%></span>
                        <% } %>
                    </div>
                    <div class="product-action-1 show">
                        <a aria-label="Add To Cart" class="action-btn hover-up" href="/cart/add/<%= product._id %>">
                            <i class="fi-rs-shopping-bag-add"></i>
                        </a>
                    </div>
                </div>
            </div>
        </div>
    <% }); %>
</div>


